<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddPunchController">
   <children>
      <TextField fx:id="timeTxt" layoutX="222.0" layoutY="281.0" />
      <TextField fx:id="dateTxt" layoutX="499.0" layoutY="281.0" />
      <Label layoutX="283.0" layoutY="248.0" text="TIME" />
      <Label layoutX="264.0" layoutY="319.0" text="(HH:MM:SS)" />
      <Label layoutX="559.0" layoutY="248.0" text="DATE" />
      <Label layoutX="532.0" layoutY="319.0" text="(YYYY-MM-DD)" />
      <Button fx:id="addPunchBtn" layoutX="55.0" layoutY="624.0" mnemonicParsing="false" onAction="#addPunch" text="Add Punch" />
      <Button fx:id="cancelBtn" layoutX="794.0" layoutY="624.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Label fx:id="timeError" layoutX="222.0" layoutY="360.0" textFill="RED" />
      <Label fx:id="dateError" layoutX="222.0" layoutY="399.0" textFill="RED" />
   </children>
</AnchorPane>
